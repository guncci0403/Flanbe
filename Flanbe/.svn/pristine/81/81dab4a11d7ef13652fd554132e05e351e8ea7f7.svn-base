package kr.or.ddit.evaluation.model;

public class EvaluationVo2 {
	private int p_code;
	private String user_id;
	private double pro;
	private double sat;
	private double comm;
	private double ontime;
	private double activity;
	private double average;
	private String e_cont;
	
	public EvaluationVo2() {}
	
	public EvaluationVo2(int p_code, String user_id, int pro, int sat, int comm, int ontime, int activity, int average,
			String e_cont) {
		super();
		this.p_code = p_code;
		this.user_id = user_id;
		this.pro = pro;
		this.sat = sat;
		this.comm = comm;
		this.ontime = ontime;
		this.activity = activity;
		this.average = average;
		this.e_cont = e_cont;
	}
	
	public int getP_code() {
		return p_code;
	}

	public void setP_code(int p_code) {
		this.p_code = p_code;
	}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public double getPro() {
		return pro;
	}

	public void setPro(double pro) {
		this.pro = pro;
	}

	public double getSat() {
		return sat;
	}

	public void setSat(double sat) {
		this.sat = sat;
	}

	public double getComm() {
		return comm;
	}

	public void setComm(double comm) {
		this.comm = comm;
	}

	public double getOntime() {
		return ontime;
	}

	public void setOntime(double ontime) {
		this.ontime = ontime;
	}

	public double getActivity() {
		return activity;
	}

	public void setActivity(double activity) {
		this.activity = activity;
	}

	public double getAverage() {
		return average;
	}

	public void setAverage(double average) {
		this.average = average;
	}

	public String getE_cont() {
		return e_cont;
	}

	public void setE_cont(String e_cont) {
		this.e_cont = e_cont;
	}

	@Override
	public String toString() {
		return "EvaluationVo [p_code=" + p_code + ", user_id=" + user_id + ", pro=" + pro + ", sat=" + sat + ", comm="
				+ comm + ", ontime=" + ontime + ", activity=" + activity + ", average=" + average + ", e_cont=" + e_cont
				+ "]";
	}
	
	
}
