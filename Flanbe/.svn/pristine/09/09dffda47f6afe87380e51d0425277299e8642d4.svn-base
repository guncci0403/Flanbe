package kr.or.ddit.note.model;


// 댓글
public class ReplyVo {

	private int r_no;			// 댓글번호(시퀀스)
	private int n_no;			// 글번호
	private String user_id;		// 작성자
	private String r_cont;		// 내용
	private String r_regdt;		// 작성일시
	private int r_gno;			// 그룹번호
	private int r_parent;		// 상위 댓글 번호
	private String r_parent_name; // 상위 댓글 아이디
	
	public ReplyVo() {}

	
	public ReplyVo(int r_no, int n_no, String user_id, String r_cont, String r_regdt, int r_gno, int r_parent,
			String r_parent_name) {
		super();
		this.r_no = r_no;
		this.n_no = n_no;
		this.user_id = user_id;
		this.r_cont = r_cont;
		this.r_regdt = r_regdt;
		this.r_gno = r_gno;
		this.r_parent = r_parent;
		this.r_parent_name = r_parent_name;
	}

	public int getR_no() {
		return r_no;
	}

	public void setR_no(int r_no) {
		this.r_no = r_no;
	}

	public int getN_no() {
		return n_no;
	}

	public void setN_no(int n_no) {
		this.n_no = n_no;
	}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public String getR_cont() {
		return r_cont;
	}

	public void setR_cont(String r_cont) {
		this.r_cont = r_cont;
	}

	public String getR_regdt() {
		return r_regdt;
	}

	public void setR_regdt(String r_regdt) {
		this.r_regdt = r_regdt;
	}

	public int getR_gno() {
		return r_gno;
	}

	public void setR_gno(int r_gno) {
		this.r_gno = r_gno;
	}

	public int getR_parent() {
		return r_parent;
	}

	public void setR_parent(int r_parent) {
		this.r_parent = r_parent;
	}

	public String getR_parent_name() {
		return r_parent_name;
	}

	public void setR_parent_name(String r_parent_name) {
		this.r_parent_name = r_parent_name;
	}

	@Override
	public String toString() {
		return "ReplyVo [r_no=" + r_no + ", n_no=" + n_no + ", user_id=" + user_id + ", r_cont=" + r_cont + ", r_regdt="
				+ r_regdt + ", r_gno=" + r_gno + ", r_parent=" + r_parent + ", r_parent_name=" + r_parent_name + "]";
	}
	
	
}
